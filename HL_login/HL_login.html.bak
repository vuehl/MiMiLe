<script type="text/javascript" src="../HLjs/jquery.js" /></script>
<script type="text/javascript" src="../HLjs/HL_login.js" /></script>
<link rel="stylesheet" type="text/css" href="../HLcss/HL_login.css" />
<body> 
<!--logo界面开始-->
<div id="HL_logo">
	<div class="HL_logo"><a href="../index.html"><img src="../HL_photos/login.png" alt="" /></a></div>
	<span class="HL_logo_info">欢迎登录</span>
</div>
<div id="HL_bg">
<!--登陆的界面开始-->
<div id="HL_login">
	<div class="HL_login">
	<span class="HL_login_count">账号登录</span>	
	<span class="HL_login_scan">扫码登录</span>	
	</div>
	<!--div盒子里class标签中HL_info是用来报登陆是否成功-->
	<form action="" method="" name="form">
	<div class="HL_toggle">
	<div class="HL_info">账号或者密码不正确！！！</div>
  <input class="HL_user" type="text" maxlength="11" alt="" placeholder="请输入账号" />
  <div class="HL_user_info">请输入账号</div>
  <input class="HL_pwd" type="password" maxlength="16" alt="" />
  <div class="HL_pwd_info">请输入密码</div>
  <div class="HL_other"><input type="checkbox" alt=""/>自动登录 <a href="#">忘记密码</a></div>
	<div class="HL_button"> <input type="button" class="button" value="登录" /></div>
	<div class="HL_infos">——————其他登录方式——————</div>
	</div>
	</form>
	<!--这个是微信登陆的图片-->
	<div class="HL_toggle_code"></div>
	<div class="HL_other_info">
		<a href="#" class="HL_other_info_QQ"><img src="../HL_photos/HL_qq.png" alt="QQ登录" /></a>
		<a href="#" class="HL_other_info_wechat"><img src="../HL_photos/HL_wechat.png" alt="微信登录" /></a>
		<span class="HL_other_item"><a href="#">立即注册</a></span>
	</div>
</div>
</div>

